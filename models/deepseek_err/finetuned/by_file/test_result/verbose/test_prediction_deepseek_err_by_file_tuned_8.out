```lean
import Mathlib.Data.Tree.Basic

namespace Tree

variable {α : Type} [Preorder α]

def validBST' : Tree α → WithBot α → WithTop α → Bool
  | nil, _, _ => true
  | node a l r, some lb, some ub =>
      (∀ x ∈ l, lb < x) && (∀ x ∈ r, x < ub) &&
      validBST' l lb (some a) && validBST' r (some a) ub
  | node a l r, _, _ =>
      (∀ x ∈ l, a < x) && (∀ x ∈ r, x < a) &&
      validBST' l (some a) (some a) && validBST' r (some a) (some a)

def validBST (t : Tree α) : Bool :=
  validBST' t none none

end Tree
```

