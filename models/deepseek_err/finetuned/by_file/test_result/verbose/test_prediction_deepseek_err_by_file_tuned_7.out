#include <stack>

struct TreeNode {
  int val;
  TreeNode *left;
  TreeNode *right;
  TreeNode(int x) : val(x), left(NULL), right(NULL) {}
};

bool isValidBST(TreeNode *root) : bool :=
  if root == NULL then
    true
  else
    let stack := []
    let prev := NULL
    let node := root
    while true do
      while node != NULL do
        stack := node :: stack
        node := node->left
      if stack.isEmpty then
        break
      node := stack.pop
      if prev != NULL && (node->val <= prev->val) then
        return false
      prev := node
      node := node->right
    true
