import Std.Data.Array.Basic

def minJumps (arr : Array Nat) : Nat :=
  let n := arr.size
  let mut dp := Array.mkArray n (Nat.succ n)
  dp := dp.set! 0 0
  for i in [1:n] do
    for j in [0:i] do
      if arr[j] >= i - j then
        if dp[j] + 1 < dp[i] then
          dp := dp.set! i (dp[j] + 1)
  dp[n - 1]

