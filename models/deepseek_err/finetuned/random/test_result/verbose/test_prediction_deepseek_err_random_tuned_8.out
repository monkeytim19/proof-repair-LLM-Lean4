import Mathlib.Data.Tree.Basic

def isValidBST (root : Tree Nat) : Bool :=
  let s1 := Stack.empty.push root
  let s2 := Stack.empty
  let lower := none
  let upper := none
  go s1 s2 lower upper
where
  go : Stack (Tree Nat) → Stack (Tree Nat) → Option Nat → Option Nat → Bool
    | Stack.empty, _, _, _ => true
    | Stack.push node stack, stack', lower, upper =>
      let nodeInRange := (lower.map (· < node.val)).getD true && (upper.map (· > node.val)).getD true
      if node.left != none then
        go (Stack.push node.left stack) (Stack.push node stack') lower (some node.val)
      else if node.right != none then
        go (Stack.push node.right stack) stack lower upper
      else
